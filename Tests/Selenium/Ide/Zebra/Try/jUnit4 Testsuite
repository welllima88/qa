		suite.addTestSuite(Standort anlegen.class);
		suite.addTestSuite(Kontakt anlegen.class);
		suite.addTestSuite(Terminal wÃ¤hlen checks.class);
		suite.addTestSuite(Terminal anlegen.class);
		suite.addTestSuite(Logout.class);
		return suite;
	}

	public static void main(String[] args) {
		junit.textui.TestRunner.run(suite());
	}
